-server
-Xmx2048m
-Xms2048m
-XX:MaxDirectMemorySize=2g
-verbose:gc
-XX:+PrintGCDetails
-XX:+PrintGCDateStamps
-XX:+ExitOnOutOfMemoryError
-Xloggc:/usr/local/druid/log/gc/router-%t.gc.log
-XX:+HeapDumpOnOutOfMemoryError
-XX:HeapDumpPath=/usr/local/druid/log/gc/router.hprof
-Duser.timezone=UTC
-Dfile.encoding=UTF-8
-Djava.io.tmpdir=var/tmp
-Djava.util.logging.manager=org.apache.logging.log4j.jul.LogManager
-Dlog4j.configurationFile=/usr/local/druid/conf/druid/router/log4j2.xml
